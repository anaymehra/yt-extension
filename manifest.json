{
  "manifest_version": 2,
  "name": "Auto Play Pause YOUTUBE",
  "version": "1.0",
  "description": "Auto Pause/Play youtube video based on tab switch",
  "permissions": ["activeTab","tabs"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
